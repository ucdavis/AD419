    <DataSet Name="ProjectsByDepartment">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     Project.Project, Project.Accession, Project.ProjTypeCd, Project.RegionalProjNum, Project.StatusCd, SUM(Associations.Expenses) AS Spent, 
                      SUM(Associations.FTE) AS FTE, Project.inv1, Project.inv2, Project.inv3, Project.inv4, Project.inv5, Project.inv6
FROM         Project INNER JOIN
                      ProjXOrgR ON Project.Accession = ProjXOrgR.Accession LEFT OUTER JOIN
                      Associations ON Project.Accession = Associations.Accession INNER JOIN
                      ReportingOrg ON ProjXOrgR.OrgR = ReportingOrg.OrgR
WHERE     (ReportingOrg.CRISDeptCd = @CRISDeptCd)
GROUP BY Project.Project, Project.Accession, Project.inv1, Project.ProjTypeCd, Project.RegionalProjNum, Project.StatusCd, Project.inv2, Project.inv3, 
                      Project.inv4, Project.inv5, Project.inv6</CommandText>
        <QueryParameters>
